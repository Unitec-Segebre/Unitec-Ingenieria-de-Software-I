<%= form_for(project) do |f| %>
<div class="form-group">
  <%= f.label :title, "Titulo" %>
  <%= f.text_field :title, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :manager, "Responsable del proyecto" %>
  <%= f.text_field :manager, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :image_url, "URL de Imagen" %>
  <%= f.text_field :image_url, class: "form-control" %>
</div>
<div class="btn-group" role="group">
  <%= f.submit "Save",class: "btn btn-default" %>
  <%= link_to "Cancel", projects_path,class: "btn btn-default" %>
</div>
<% end %>
