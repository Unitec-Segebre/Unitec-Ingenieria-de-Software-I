<div class = "wrapper">
  <div class = "first">
    <h1>Lotes de <i><%=@project.title %></i></h1>
    <h4>Empleado encargado: <%=@project.manager%></h4>
  </div>
  <div class = "second">
    <% unless @project.image_url.blank? %>
      <%=image_tag @project.image_url,:class=>"img_style", :title => "Click aqui para ver los lotes del proyecto"%>
    <%end%>
  </div>
</div>
<%=render partial: "lots/index"%>
