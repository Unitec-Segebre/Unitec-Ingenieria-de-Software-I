
<div class="col-md-3">
  <div class="panel panel-green">
    <div class="panel-heading">
      <strong>
        <%= project.title %>
      </strong>
    </div>
    <div class="panel-body">
      <%unless project.image_url.blank? %>
        <%=link_to(image_tag(project.image_url,:class=>"img_style", :title => "Click aqui para ver los lotes del proyecto"),project)%>
      <%else%>
        <%=link_to(image_tag("project_example.jpeg",:class=>"img_style", :title => "Click aqui para ver los lotes del proyecto"),project)%>
      <%end%>
    </div>
  </div>
</div>
